#include <stdio.h>

int isLeapYear(int n)
{
    if ((n % 4 == 0 && n % 100 != 0) || n % 400 == 0)
        return 1;
    return 0;
}

int main()
{
    int n;
    scanf("%d", &n);
    if (isLeapYear(n) == 1)
        printf("%d is a leap year!\n", n);
    else
        printf("%d is not a leap year!\n", n);
}
